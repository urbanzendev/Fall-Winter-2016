<script type="text/javascript">
 
$(document).ready(function() {
	$('.zoom-container').click(function(event) {
		$('.shop-the-look-container.previousDrawer').slideUp('slow');
		$('.shop-the-look-container.previousDrawer').removeClass('previousDrawer');
		$('.shop-the-look-container').empty();
		$('.close').remove();
		var shopTheLookClass = $(this).attr('shop-the-look');
		var link = '/collections/all/'+ shopTheLookClass;
		var getUrl = link;
		$.ajax({
			url: getUrl,
			type:'GET',
			success: function(data){
				$(event.target).closest('.image-row').find('.shop-the-look-container').slideDown('slow').addClass('previousDrawer').html($(data).find('.products').html());
				$(event.target).closest('.image-row').find('.shop-the-look-container').append('<a href="#" class="close">close</a>');
				$("img").unveil(300).css('opacity', 1);
				if(shopTheLookClass = "shop-the-look-1"){
					$("[color='Black Viscose']").remove(); 	
				}	
				$('.close').click(function(event){
					console.log('clicked close');
					$(this).closest('.image-row').find('.shop-the-look-container').slideUp('slow');
					$(this).closest('.image-row').find('.shop-the-look-container').empty();	
				});
				{% include 'addToCart-ajax' %}
			}
		});
		
	});
	
});
</script>



