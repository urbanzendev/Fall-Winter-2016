<script>
$(document).ready(function() {
		var accordion = $('.accordion > dd').hide();
		var submenu = $('.accordion-sub > dd').hide();
	$('.open').on('touchstart click',function(e) {
		e.preventDefault();
		$this = $(this);
		$target =  $this.parent().next();
		if(!$this.hasClass('close')){
			console.log($this);
			$('dt').find('small.right').html('<span class="down-angle"></span>');
			$('.open').removeClass('close');
			accordion.slideUp();
			$this.addClass('close');
			$target.slideDown();
			$(this).parent('dt').find('small.right').html('<span class="up-angle"></span>');
		}else{
			console.log('clicked Closed!');
			$target.slideUp();
			$this.removeClass('close');
			$('dt').find('small.right').html('<span class="down-angle"></span>');
		}
		return false;
	});
	
	$('.sub-open').on('touchstart click',function(e) {
		console.log('sub clicked!');
		e.preventDefault();
		$this = $(this);
		$subTarget =  $this.parent().next();
		if(!$this.hasClass('close')){
			console.log('accordion-sub');
			$('dt').find('small.right').html('<span class="down-angle"></span>');
			$('.sub-open').removeClass('close');
			submenu.slideUp();
			$this.addClass('close');
			$subTarget.slideDown();
			$(this).parent('dt').find('small.right').html('<span class="up-angle"></span>');
		}else{
			console.log('clicked Closed!');
			$subTarget.slideUp();
			$this.removeClass('close');
			$('dt').find('small.right').html('<span class="down-angle"></span>');
		}
		return false;
	});
	$('.storesSlider').flexslider({
		animation: "slide",
		animationLoop: false,
		slideshow:false,
		directionNav:true,
		prevText:'',
		nextText:'',
		start:function() {},
		manualControls:".accordion .open"
	});
});
</script>